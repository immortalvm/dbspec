# Expected exit status code: 0

Parameters:
	connection_string

Set conn = connection to connection_string

Output conn to "my.siard"

Set res = result via conn:
	SELECT * FROM trace
Assert res.size == 1

Set res = result via conn:
	SELECT 1
	FROM trace
	WHERE id = 1
	AND (data)."type" = 'extraction'
	AND (data)."url" = 'jdbc:h2:mem:test'
	AND (data)."filename" = 'my.siard'
Assert res.size == 1
